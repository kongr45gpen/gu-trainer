	-disableBots
	+r
	-sl g 1
	-sl gm 15
	-sl l 15
	-ms 4
	-set _reloadTime 2.5
	-sb
	-set _wingsJumpCount 5
	-set _mGunAdVel 6
	-sl SB 15
	-sl SW 10
