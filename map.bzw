	-lagdrop 3
	-lagwarn 350/10
	-tkannounce
	-set _thiefAdShotVel 10
	-set _wallHeight 800
end

define Level
#A wall at the edge of the world will not ricochet
	mesh
		vertex -400 400 0
		vertex 400 400 0
		vertex 400 -400 0
		vertex -400 -400 0
#		vertex -400 400 6.15
#		vertex 400 400 6.15
#		vertex 400 -400 6.15
#		vertex -400 -400 6.15
		face Floor
			texture std_ground
			vertices 3 2 1 0
		endface
		face Ceiling
			texture mesh
			vertices 0 1 2 3
		endface
#		face Wall 1
#			texture wall
#			vertices 0 1 5 4
#		endface
#		face Wall 2
#			texture wall
#			vertices 1 2 6 5
#		endface
#		face Wall 3
#			texture wall
#			vertices 2 3 7 6
#		endface
#		face Wall 4
#			texture wall
#			vertices 3 0 4 7
#		endface
	end
enddef

